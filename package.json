{
  "name": "cdktf-diff-action",
  "version": "1.0.0",
  "author": "@snapsheet",
  "private": true,
  "description": "GitHub Action to execute cdktf diff and parse outputs",
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "actions",
    "node",
